swiper-item>view{
	width: 615rpx;
	height: 900rpx;
	background: linear-gradient(140deg,#3d3a35,#292826); 
	margin:0 auto ;
	border-radius: 20rpx;
	color: #fff;
	text-align: center;
}
swiper-item .mask{
	position: fixed;
	width: 630rpx;
	height: 900rpx;
	top: 0;
	background: rgba(255,255,255,0.2);
	z-index: 99;
}
.cu-avatar{
	width: 200rpx;
	height:180rpx;
	margin: 100rpx 200rpx 20rpx;
	background-image:url('data:image/jpeg;base64,');
	font-size: 24rpx;
	padding-top:115rpx ;
}
.points{
	font-size: 50rpx;
}
.mypoints{
	font-size: 22rpx;
}
.myright{
	background-color: #444341;
	width: 92%;
	height: 150rpx;
	margin:30rpx 4%;
	border-radius: 15rpx;
}
.myright>view{
	float: left;
	margin: 45rpx 10rpx 0 45rpx;
}
.icon{
	font-size: 35rpx;
}
.detail{
