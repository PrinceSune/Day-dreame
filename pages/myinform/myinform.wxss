page{
    background-color: #FFFFFF;
    height: 100%;
}
.liebiao view{
	border-bottom:1rpx solid #ddd;
	height: 100rpx;
	width: 100%;
	line-height: 100rpx;
	font-size: 25rpx;
}
.liebiao text{
	color: 8E8E8E;
